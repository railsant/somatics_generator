<table width="100%">
<tbody>
	<tr>
		<td colspan="2"><br/><h3><%%=<%=class_name%>.human_name%></h3></td>
	</tr>
<% for attribute in attributes -%>
	<tr>
	  <td><b><%%= <%= class_name %>.human_attribute_name(:<%=attribute.name%>)%></b></td>
	  <td><%%= f.<%= attribute.field_type %> :<%= attribute.name %> %></td>
	</tr>
<% end -%>
</tbody>
</table>